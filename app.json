{
  "expo": {
    "name": "hoidetmay",
    "slug": "hoidetmay",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maymaygiangthanh.app",
      "buildNumber": "1.0.9",
      "infoPlist": {
        "NSCameraUsageDescription": "This application uses the camera to take photos of people when taking attendance",
        "NSPhotoLibraryAddUsageDescription": "The application uses images to make avatar images",
        "NSPhotoLibraryUsageDescription": "The application uses images to make avatar images"
      }
    },
    "android": {
      "package": "com.hoidetmay.app",
      "versionCode": 2,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
  }
  }
}
